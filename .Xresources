!! Colorscheme

!xterm*decTerminalID:            vt340

!URxvt.perl-ext-common: sixel-proof-of-concept


! special
URxvt.scrollBar:false


URxvt*background: Black
URxvt*foreground: #CCCCCC
!URxvt*geometry: 80x30

!URxvt.font: 9x15

!-- Xft settings -- !
!Xft.dpi:        96
!Xft.antialias:  true 
!Xft.rgba:       rgb
!Xft.hinting:    true
!Xft.hintstyle:  hintfull
!
!! -- Fonts -- !

!https://blog.khmersite.net/2017/12/change-the-urxvt-font-size-on-the-fly/
Urxvt.perl-ext-common: default,resize-font,tabbed


!tabbed
URxvt.tabbed.tabbar-fg: 2
URxvt.tabbed.tabbar-bg: 0
URxvt.tabbed.tab-fg:    3
URxvt.tabbed.tab-bg:    0



URxvt.letterSpace:              -1
URxvt.lineSpace:              -1

URxvt.font:                     xft:D2Coding:style=Book:pixelsize=17
URxvt.boldfont:                 xft:D2Coding:style=Bold:pixelsize=17


!urxvt*color0:         #101010
!urxvt*color8:         #525252
!
!urxvt*color1:        #e158b8
!urxvt*color9:        #e158b8
!
